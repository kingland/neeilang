fn main() : Int {
  var myFloat : Float = 3.5;

  if (myFloat > 1) {
    print "myFloat 3.5 is bigger than the integer 1";
  } else {
    print "myFloat 3.5 is less than the integer 1";
  }
  

  // Add a Float to an Int
  var myInt : Int = 5;
  print (myFloat + myInt + 1); // 9.5

  if (3 >= 3.0) {
    print "3 >= 3.0";
  }

  return 0;
}
