fn main() : Int {
  var a : Int = 5;
  var b : Int = 3;

  print a + b;    // 8
  print a - b;    // 2
  print b - a;    // -2

  if (a >= 5) {
    print "a >= 5"; // a >= 5
  }

  if (a > 5) {
    print "a > 5";  // Not executed
  }

  if (a == b) {
    print "a == b"; // Not executed
  }

  if (b != a) {
    print "b != a"; // b != a
  }

  print a * b;    // 15

  print a / b;    // 1
  print b / a;    // 0
  print 120 / 9;  // 13

  return 0;
}

